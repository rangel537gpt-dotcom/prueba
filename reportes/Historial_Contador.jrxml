	<queryString>
		<![CDATA[select Convert(varchar, DocumentoPago.FechaPago) AS FechaPago, ConceptoPagoDetalle.TipoCodigo,DocumentoPago.NroSerie,
RIGHT('0000000' + Ltrim(Rtrim(DocumentoPago.NroDocumentoPago)),7) as NroDocumentoPago, Cliente.CCodigoCole, DocumentoPago.NombreCliente AS 'Nombre o Razón Social',
CONCAT (ConceptoPagoDetalle.TipoCodigo, ConceptoPagoDetalle.Codigo) AS Evento,
isnull(ConceptoPagoDetalle.Descripcion,'') + ' ' + isnull(DocumentoPagoDet.Observacion,'') AS Descripcion,
DocumentoPagoDet.Cantidad, DocumentoPagoDet.ValorVenta + DocumentoPagoDet.Igv as Importe
from DocumentoPago, ConceptoPagoDetalle, Cliente, DocumentoPagoDet
where DocumentoPagoDet.ValorVenta + DocumentoPagoDet.Igv <> 0
and Cliente.IdCliente = DocumentoPago.IdCliente
and DocumentoPago.IdDocumentoPago = DocumentoPagoDet.IdDocumentoPago
and ConceptoPagoDetalle.IdConceptoPagoDetalle = DocumentoPagoDet.IdConceptoPagoDetalle
and DocumentoPago.IdCliente = $P{IDCLIENTE}
Order by DocumentoPago.FechaPago, DocumentoPago.NroSerie, DocumentoPago.NroDocumentoPago]]>
	</queryString>
