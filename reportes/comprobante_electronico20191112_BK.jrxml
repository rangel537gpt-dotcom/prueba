	<parameter name="PARTICIPANTES" class="java.lang.Integer">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT

CASE
WHEN Cliente.TipoCliente = 'C'
	THEN CONCAT (Cliente.PNombre, + ' ' +  Cliente.PApePat, + ' ' +  Cliente.PApeMat)
WHEN Cliente.TipoCliente = 'P'
	THEN CONCAT (Cliente.PNombre, + ' ' +  Cliente.PApePat, + ' ' +  Cliente.PApeMat)
WHEN Cliente.TipoCliente = 'E'
	THEN Cliente.ENombre
WHEN Cliente.TipoCliente = 'S'
	THEN Cliente.SNombreSociedad
	ELSE '' END
AS 'CLIENTE',

CASE
WHEN Cliente.TipoCliente = 'C'
	THEN Cliente.PNroDocumento + ' / MATRICULA:' + Cliente.CCodigoCole
WHEN Cliente.TipoCliente = 'P'
	THEN Cliente.PNroDocumento
WHEN Cliente.TipoCliente = 'E'
	THEN Cliente.ERUC
WHEN Cliente.TipoCliente = 'S'
	THEN Cliente.SRUC + ' / MATRICULA:' + Cliente.SCodigoSoc
	ELSE '' END
AS 'DOCUMENTO',

CASE
WHEN Cliente.TipoCliente = 'C'
	THEN Cliente.PDireccionDomicilio
WHEN Cliente.TipoCliente = 'P'
	THEN Cliente.PDireccionDomicilio
WHEN Cliente.TipoCliente = 'E'
	THEN Cliente.EDireccion
WHEN Cliente.TipoCliente = 'S'
	THEN Cliente.SDireccion
	ELSE '' END
AS 'DIRECCION',

CASE
WHEN Cliente.TipoCliente = 'C'
	THEN 1
WHEN Cliente.TipoCliente = 'P'
	THEN 1
WHEN Cliente.TipoCliente = 'E'
	THEN 6
WHEN Cliente.TipoCliente = 'S'
	THEN 6
	ELSE '' END
AS 'TIPO DOC ADQ',


CASE
WHEN DocumentoPago.Moneda = 'S'
	THEN 'SOLES'
ELSE 'DOLARES'
END AS 'MONEDA',
RIGHT('000000' + Ltrim(Rtrim(DocumentoPago.NroDocumentoPago)),6) AS 'NRO DOC PAGO', DocumentoPagoDet.Cantidad, DocumentoPagoDet.ValorVenta,DocumentoPagoDet.PrecioVenta, DocumentoPagoDet.Igv,
DocumentoPago.FechaSunat, DocumentoPago.NroSerie, ConceptoPagoDetalle.Descripcion + ISNULL(' / PARTICIPANTES:' + STUFF((SELECT ' , '  + Alumnos.ApellidoPaterno +  ' ' + Alumnos.ApellidoMaterno + ' ' + Alumnos.Nombres
    FROM Alumnos
    WHERE Alumnos.IdDocumentoPagoDet = DocumentoPagoDet.IdDocumentoPagoDet AND 1 = $P{PARTICIPANTES}
    FOR XML PATH('')),1,2,''),'') AS Descripcion,
DocumentoPago.NroSerie as 'SERIE',
DocumentoPagoDet.PrecioUnitario AS 'Valor Unitario',
ISNULL(DocumentoPagoDet.OBSERVACION,'') AS OBSERVACION,
ISNULL(DocumentoPagoDet.Glosa,'') AS GLOSA
FROM DocumentoPagoDet
INNER JOIN DocumentoPago ON DocumentoPagoDet.IdDocumentoPago = DocumentoPago.IdDocumentoPago
INNER JOIN Cliente ON DocumentoPago.IdCliente = Cliente.IdCliente
INNER JOIN ConceptoPagoDetalle ON DocumentoPagoDet.IdConceptoPagoDetalle = ConceptoPagoDetalle.IdConceptoPagoDetalle
INNER JOIN TipoDocSerie ON TipoDocSerie.Id = DocumentoPago.IdTipoPagoSerie
INNER JOIN TipoDocPago ON TipoDocSerie.IdTipoDoc= TipoDocPago.IdTipoDocPago
INNER JOIN Serie ON TipoDocSerie.IdSerie = Serie.IdSerie
WHERE DocumentoPago.IdDocumentoPago = $P{IDDOCUMENTOPAGO}]]>
	</queryString>
