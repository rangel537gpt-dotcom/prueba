	<queryString>
		<![CDATA[SELECT DocumentoPago.TipoPago,Serie.Serie,TipoDocPago.NombreDocPago,DocumentoPago.NroDocumentoPago,DocumentoPago.Moneda,DocumentoPago.FechaPago
FROM DocumentoPago,Serie,TipoDocPago,TipoDocSerie
WHERE DocumentoPago.IdTipoPagoSerie = TipoDocSerie.Id AND
TipoDocSerie.IdSerie = Serie.IdSerie AND
TipoDocSerie.IdTipoDoc = TipoDocPago.IdTipoDocPago AND
YEAR(DocumentoPago.FechaPago) = $P{ANIO} AND
MONTH(DocumentoPago.FechaPago) = $P{MES} AND
DAY(DocumentoPago.FechaPago) = $P{DIA} AND
DocumentoPago.ESTADO='ANULADO'
ORDER BY TipoDocPago.NombreDocPago,Serie.Serie,DocumentoPago.NroDocumentoPago]]>
	</queryString>
